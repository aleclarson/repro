{
  "include": [
    "src"
  ],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "noErrorTruncation": true,
    "outDir": "lib",
    "preserveSymlinks": true,
    "strict": true,
    "target": "es5"
  }
}
