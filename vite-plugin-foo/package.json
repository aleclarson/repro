{
  "name": "vite-plugin-foo",
  "version": "0.0.0",
  "author": "Alec Larson",
  "license": "MIT",
  "main": "dist/node/plugin.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "concurrently 'yarn build:client -w' 'yarn build:node -w'",
    "build": "concurrently npm:build:client npm:build:node",
    "build:client": "tsc -p ./src/client",
    "build:node": "tsc -p ./src/node",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "vite": ">2.0.0-0"
  },
  "devDependencies": {
    "@alloc/prettier-config": "^1.0.0",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "concurrently": "^6.0.0",
    "prettier": "^2.0.5",
    "typescript": "^4.0.0",
    "vite": "latest"
  },
  "prettier": "@alloc/prettier-config",
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
